   "source": [
    "\n",
    "def cantore(n : int = N):\n",
    "    if n == 0:\n",
    "        return '-'\n",
    "    tn = 3 ** (n - 1)\n",
    "    return cantore(n-1) + ' ' * tn + cantore(n-1)\n",
    "\n",
    "cantore(N)\n"
   ]
  }
